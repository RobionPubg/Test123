                W2S(foot_l, (FVector2D*)&foot_lPoSC) &&
                W2S(thigh_r, (FVector2D*)&thigh_rPoSC) &&
                W2S(calf_r, (FVector2D*)&calf_rPoSC) &&
                W2S(foot_r, (FVector2D*)&foot_rPoSC) &&
                W2S(neck_01, (FVector2D*)&neck_01PoSC) &&
